<h1>Create Heroine</h1>
<% if flash.now[:error] %>
<% flash.now[:error].each do |error_msg|%>
  <li><%= error_msg %></li>
  <% end %>
<% end %>

<%= form_for @heroine do |f|%><br>
<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.label :super_name %>
<%= f.text_field :super_name%><br>
<%= f.collection_select :power_id, @powers, :id, :name %>
<%= f.submit %>
<% end %>
