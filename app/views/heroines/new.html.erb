<%  if flash[:error] %>
<ul>
    <% @heroine.errors.each do |err| %>
        <li><%= err %> </li>
    <% end%>
</ul>
<% end%>


<h1> New Heroine</h1>

<%= form_for @heroine do |f|%>
<%= f.label :name %>
<%= f.text_field :name %> <br>
<%= f.label :super_name %>
<%= f.text_field :super_name %> <br>
<%= f.collection_select  :power_id, @powers, :id, :name%>
<%= f.submit %>
<% end%>